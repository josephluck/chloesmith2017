{
  "name": "chloesmith2017",
  "version": "1.0.0",
  "description": "Portfolio site",
  "main": "dist/index.js",
  "scripts": {
    "dev": "budo ./src/index.ts --live --pushstate --host=localhost --port=8888 --base=/ --serve=index.js -- -t [ babelify ] -t [ scssify ] -p [ tsify ]",
    "dist": "tsc && npm run browserify && npm run minify && npm run assets",
    "assets": "cp -R assets dist && cp index.html dist/index.html",
    "browserify": "browserify src/index.ts -p [ tsify ] -t [ scssify ] -g babelify -g uglifyify -o dist/index.js",
    "minify": "uglifyjs ./dist/index.js --compress --mangle -o ./dist/index.js"
  },
  "dependencies": {
    "@types/node": "^7.0.5",
    "babel": "^6.23.0",
    "babel-cli": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "budo": "^9.4.7",
    "eases": "^1.0.8",
    "helix-yo-yo": "1.1.0",
    "scssify": "^2.2.1",
    "tachyons": "^4.6.2",
    "tsify": "^3.0.1",
    "tslint": "^4.4.2",
    "tslint-config-standard": "^4.0.0",
    "typescript": "^2.2.1",
    "uglifyify": "^3.0.4",
    "uglifyjs": "^2.4.10"
  },
  "author": "",
  "license": "ISC"
}
